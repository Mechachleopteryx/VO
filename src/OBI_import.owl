        <obo:IAO_0000111 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">antigen role</obo:IAO_0000111>
        <obo:IAO_0000115 xml:lang="en">Antigen is a role played by material which when introduced into an immune-competent organism causes an immune response</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">Role Branch</obo:IAO_0000117>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000123"/>
